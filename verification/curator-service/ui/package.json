{
    "name": "ui",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@date-io/date-fns": "^3.0.0",
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.5",
        "@material-table/core": "^6.4.1",
        "@mui/icons-material": "^5.10.3",
        "@mui/lab": "^5.0.0-alpha.170",
        "@mui/material": "^5.15.15",
        "@mui/system": "^5.15.15",
        "@mui/x-date-pickers": "^7.1.1",
        "@reduxjs/toolkit": "^1.8.5",
        "@typescript-eslint/eslint-plugin": "^7.6.0",
        "@vitejs/plugin-react": "^4.2.1",
        "axios": "^0.26.1",
        "clsx": "^1.1.1",
        "css-mediaquery": "^0.1.2",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "envalid": "^8.0.0",
        "formik": "^2.4.5",
        "formik-mui": "^5.0.0-alpha.0",
        "formik-mui-lab": "^1.0.0",
        "formik-mui-x-date-pickers": "^0.0.1",
        "history": "^4.10.1",
        "http-proxy-middleware": "^2.0.1",
        "i18n-iso-countries": "^7.11.0",
        "lodash": "^4.17.21",
        "msw": "^0.39.2",
        "papaparse": "^5.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-draggable": "^4.4.6",
        "react-google-button": "^0.7.2",
        "react-google-recaptcha": "^3.0.0-alpha.1",
        "react-gtm-module": "^2.0.11",
        "react-helmet": "^6.1.0",
        "react-highlight-words": "^0.20.0",
        "react-password-strength-bar": "^0.4.1",
        "react-redux": "^8.0.1",
        "react-router-dom": "^6.22.3",
        "react-scroll": "^1.9.0",
        "shortid": "^2.2.8",
        "tss-react": "^4.9.6",
        "typeface-roboto": "^1.1.13",
        "vite": "^4.5.3",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^4.3.2",
        "yup": "^1.4.0"
    },
    "scripts": {
        "start-noenv": "vite",
        "start": "PORT=3002 vite",
        "build": "vite build",
        "prepare-variants": "csvtojson ./variantsofconcern.csv > src/components/assets/variants.json",
        "init-db": "npm --prefix ../../../data-serving/scripts/setup-db run migrate",
        "import-case-data": "npm --prefix ../../../data-serving/scripts/setup-db run import-sample-data",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "test": "vitest",
        "test-and-coverage": "vitest --coverage --watchAll=false --transformIgnorePatterns 'node_modules/(?!(headers-polyfill/lib)/)'",
        "test-silent": "vitest --watchAll=false",
        "eject": "vite eject",
        "cypress-open": "cypress open",
        "cypress-run": "cypress run --spec \"cypress/e2e/components/LandingPage.spec.ts\"",
        "cypress-run-ff": "cypress run --browser firefox",
        "cypress-run-edge": "cypress run --browser edge",
        "cypress-run-chrome": "cypress run --headless --browser chrome"
    },
    "jest": {
        "preset": "ts-jest/presets/js-with-ts",
        "testEnvironment": "jest-environment-jsdom",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
        },
        "modulePathIgnorePatterns": [
            "<rootDir>/cypress/"
        ],
        "modulePaths": [
            "<rootDir>/src"
        ],
        "resetMocks": false,
        "transform": {
            "^.+\\.tsx?$": [
                "ts-jest",
                {
                    "diagnostics": {
                        "ignoreCodes": [
                            1343
                        ]
                    },
                    "astTransformers": {
                        "before": [
                            {
                                "path": "node_modules/ts-jest-mock-import-meta",
                                "options": {
                                    "metaObjectReplacement": {
                                        "env": {
                                            "VITE_APP_MAP_LINK_LOCAL": "mocked_policy_public_id",
                                            "VITE_APP_MAP_LINK_LOCAL_E2E": "mocked_policy_public_id",
                                            "VITE_APP_MAP_LINK_DEV": "mocked_policy_public_id",
                                            "VITE_APP_MAP_LINK_QA": "mocked_policy_public_id",
                                            "VITE_APP_MAP_LINK_PROD": "mocked_policy_public_id",
                                            "VITE_APP_DISEASE_NAME": "mocked_policy_public_id",
                                            "SERVICE_ENV": "mocked_policy_public_id"
                                        }
                                    }
                                }
                            }
                        ]
                    }
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/dom": "^8.20.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.2.2",
        "@testing-library/user-event": "^14.4.3",
        "@types/css-mediaquery": "^0.1.1",
        "@types/jest": "^24.9.1",
        "@types/lodash": "^4.17.0",
        "@types/mongodb": "^3.6.18",
        "@types/node": "^17.0.29",
        "@types/papaparse": "^5.3.14",
        "@types/react": "^18.2.75",
        "@types/react-dom": "^18.0.6",
        "@types/react-google-recaptcha": "^2.1.5",
        "@types/react-gtm-module": "^2.0.1",
        "@types/react-helmet": "^6.1.4",
        "@types/react-highlight-words": "^0.16.3",
        "@types/react-router-dom": "^5.3.1",
        "@types/react-scroll": "^1.8.10",
        "@types/shortid": "^0.0.32",
        "@types/yup": "^0.32.0",
        "@typescript-eslint/parser": "^7.6.0",
        "@vitest/coverage-v8": "^0.34.6",
        "csvtojson": "^2.0.10",
        "cypress": "^13.2.0",
        "cypress-file-upload": "^5.0.8",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^4.3.8",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.4.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-environment-jsdom-fifteen": "^1.0.2",
        "lint-staged": "^10.4.0",
        "mongodb": "^4.9.1",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-jest-mock-import-meta": "^1.2.0",
        "typescript": "^5.4.5",
        "vitest": "^0.34.6",
        "vitest-canvas-mock": "^0.3.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*/**/*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "engines": {
        "node": "18.18.0"
    }
}
